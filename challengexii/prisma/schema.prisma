generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  fistName String
  lastName String
  email String
  country String
  city String
  referralCode String
  typeCar String
  @@map("user_table")
}